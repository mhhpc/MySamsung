<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MySamsung" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryVariant">@color/colorPrimaryDark</item>
        <item name="colorOnPrimary">@color/ColorWhite</item>
        <item name="android:forceDarkAllowed" tools:ignore="NewApi">true</item>
        <item name="android:navigationBarColor" tools:ignore="NewApi">@color/colorPrimary</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Font. -->
        <item name="fontFamily">@font/font</item>
        <item name="android:fontFamily">@font/font</item>
    </style>

    <style name="soon_toast">
        <item name="stTextBold">true</item>
        <item name="stTextColor">#FFFFFF</item>
        <item name="stFont">@font/font</item>
        <item name="stTextSize">14sp</item>
        <item name="stColorBackground">#B88A00</item>
        <item name="stSolidBackground">false</item>
        <item name="stStrokeWidth">2dp</item>
        <item name="stStrokeColor">@color/colorPrimary</item>
        <item name="stIconStart">@drawable/ic_coming_soon</item>
        <item name="stLength">LONG</item>
        LONG or SHORT
        <item name="stGravity">center</item>
        top or center
        <item name="stRadius">10dp</item>
    </style>

